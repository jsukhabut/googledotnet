<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Configuration.Google.SecretsManager</AssemblyName>
    <Version>0.9.0</Version>
    <Authors>James Sukhabut</Authors>
    <Company />
    <Product />
    <Description>Microsoft.Extensions.Configuration provider for Google Cloud Secret Manager</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/jsukhabut/googledotnet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/jsukhabut/googledotnet</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>Configuration;Extensions;Google;GCloud;Secrets Manager</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Cloud.SecretManager.V1" Version="1.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="2.2.0" />
  </ItemGroup>

</Project>
